import java.util.*;
public class Main {
public static void main(String args[])
{
	
	Scanner sc=new Scanner(System.in);
	int n=sc.nextInt();                           		//reading the number from user
	int k=0;	                                   	//initializing the variable k with 0
	for(int i=1;i<=10;i++)
	{
		k=i*n;
		System.out.println(i+"*"+n+"="+k);         	//printing the table of the number
	}	
	
}
}
